# Database connection examples for different databases

# PostgreSQL
DB_CONN_POSTGRES=postgresql://username:password@localhost:5432/database_name

# MySQL
DB_CONN_MYSQL=mysql://username:password@localhost:3306/database_name

# SQLite (local file)
DB_CONN_SQLITE=sqlite:///path/to/database.db

# SQL Server
DB_CONN_SQLSERVER=mssql+pyodbc://username:password@server:1433/database?driver=ODBC+Driver+17+for+SQL+Server

# Default settings
DEFAULT_COUNT=10
DEFAULT_FORMAT=json
DEFAULT_SEED=42
